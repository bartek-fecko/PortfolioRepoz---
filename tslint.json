{
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {
      "class-name": true,
      "comment-format": [
          true,
          "check-space"
      ],
      "indent": [
          true,
          "spaces"
      ],
      "no-duplicate-variable": true,
      "no-eval": true,
      "no-trailing-whitespace": true,
      "no-unsafe-finally": true,
      "one-line": [
          true,
          "check-open-brace",
          "check-whitespace"
      ],
      "quotemark": [
          true,
          "single"
      ],
      "semicolon": [
          true,
          "always"
      ],
      "triple-equals": [
          true,
          "allow-null-check"
      ],
      "variable-name": [
          true,
          "ban-keywords"
      ],
      "whitespace": [
          true,
          "check-branch",
          "check-decl",
          "check-operator",
          "check-separator",
          "check-type"
      ]
  },
  "rules": {
      "quotemark": [
          true,
          "single",
          "jsx-double"
      ],
      "semicolon": [
          true,
          "always"
      ],
      "triple-equals": [
          true,
          "allow-null-check"
      ],
      "variable-name": [
          true,
          "ban-keywords"
      ],
      "interface-name": [
          false,
          "never-prefix"
      ],
      "radix": false
  }
}